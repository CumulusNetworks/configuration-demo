
auto lo
iface lo inet loopback
    address fd00::11/128

auto eth0
iface eth0 inet dhcp


# uplinks
auto swp51
iface swp51
    address fd01::1/127

auto swp52
iface swp52
    address fd01::3/127


# bridge to hosts
auto swp1
iface swp1

auto br0
iface br0
     bridge-ports swp1
     address fd16::1:1/112
